.page-header
  %h1= t '.title', :default => t("pages.header.headline")

.table-responsive
  %table.table.table-striped.table-hover.table-condensed
    %thead
      %tr
        %th= t("pages.header.places")
    %tbody
      - @places.each do |place|
        %tr
          %td
            = place.title

.table-responsive
  %table.table.table-striped.table-hover.table-condensed
    %thead
      %tr
        %th= t("pages.header.routes")
    %tbody
      - @routes.each do |route|
        %tr
          %td
            = route.title
